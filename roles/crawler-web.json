{
  "chef_type": "role",
  "override_attributes": {
    "nginx": {
        "template": "gunicorn-proxy/default-site.erb"
    }
  },
  "default_attributes": {
    "nginx": {
        "gunicorn_port": 8000
    },
    "celery": {
        "virtualenv": "/opt/rdc-web-crawler/.crawler-venv",
        "beat": false,
        "workers": 2
    },
    "crawler": {
        "ssh_keys": ["ssh-rsa  crawler insecure public key"],
        "develop": false,
        "log_level": "info",
        "gunicorn_worker_class": "gevent",
        "gunicorn_workers": 2
    }
  },
  "description": "Crawler web UI worker",
  "env_run_lists": {
  },
  "name": "crawler-web",
  "run_list": [
      "recipe[apt]", "recipe[crawler::web]", "recipe[nginx]"
  ],
  "json_class": "Chef::Role"
}
