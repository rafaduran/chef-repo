{
  "chef_type": "role",
  "override_attributes": {
    "celery": {
        "command": "/opt/rdc-web-crawler/tools/with_venv.sh python /opt/rdc-web-crawler/rdc_crawler/manage.py celeryd",
        "user": "crawler",
        "group": "crawler"
    }
  },
  "default_attributes": {
    "celery": {
        "virtualenv": "/opt/rdc-web-crawler/.crawler-venv",
        "beat": false,
        "workers": 2
    },
    "crawler": {
        "ssh_keys": ["ssh-rsa  crawler insecure public key"],
        "develop": false
    }
  },
  "description": "Crawler/Celery worker",
  "env_run_lists": {
  },
  "name": "crawler-worker",
  "run_list": [
      "recipe[apt]", "recipe[crawler]", "recipe[celery]"
  ],
  "json_class": "Chef::Role"
}
